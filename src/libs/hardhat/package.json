{
  "name": "dapp-boilerplate-hardhat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "contract:run": "hardhat run scripts/run.js",
    "contract:deploy": "hardhat run scripts/deploy.js --network rinkeby",
