  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@typechain/ethers-v5": "^10.0.0",
    "@typechain/hardhat": "^6.0.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.1",
    "chai": "^4.3.6",
    "child_process": "^1.0.2",
    "dotenv": "^16.0.0",
    "ethereum-waffle": "^4.0.0-alpha.0",
    "hardhat": "^2.9.1",
    "typechain": "^8.0.0",
    "typescript": "4.4.4"
  }
}
